<?xml version="1.0" encoding="utf-8"?>
<widget id="io.cordova.zotibs" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <name>Zotibs</name>
  <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
  <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
  <content src="index.html" />
  <plugin name="cordova-plugin-whitelist" version="1" />
  <access origin="*" />
  <allow-intent href="http://*/*" />
  <allow-intent href="https://*/*" />
  <allow-intent href="tel:*" />
  <allow-intent href="sms:*" />
  <allow-intent href="mailto:*" />
  <allow-intent href="geo:*" />
  <allow-navigation href="https://cordovahostedweb.azurewebsites.net" />
  <preference name="windows-target-version" value="10.0" />
  <platform name="android">
    <allow-intent href="market:*" />
  </platform>
  <platform name="ios">
    <allow-intent href="itms:*" />
    <allow-intent href="itms-apps:*" />
  </platform>
  <vs:plugin name="cordova-plugin-whitelist" version="1.0.0" xmlns:vs="http://schemas.microsoft.com/appx/2014/htmlapps" />
  <plugin name="cordova-plugin-camera" version="2.2.0" />
  <plugin name="cordova-plugin-compat" version="1.0.0" />
  <engine name="android" spec="~5.1.1" />
  <engine name="ios" spec="~4.1.1" />
  <engine name="windows" spec="~4.3.2" />
  <plugin name="cordova-plugin-splashscreen" version="4.0.1" />
  <plugin name="phonegap-plugin-push" version="1.8.4" />
  <plugin name="cordova-plugin-statusbar" version="2.2.1" />
  <plugin name="cordova-plugin-media-capture" version="1.4.1" />
  <plugin name="cordova-plugin-code-push" version="1.9.2-beta" />
  <plugin name="cordova-plugin-console" version="1.0.5" />
  <plugin name="cordova-plugin-contacts" version="2.2.1" />
  <plugin name="cordova-plugin-device-orientation" version="1.0.5" />
  <plugin name="cordova-plugin-geolocation" version="2.4.1" />
  <plugin name="cordova-plugin-inappbrowser" version="1.6.1" />
  <plugin name="code-push" version="1.9.0-beta" />
  <plugin name="cordova-plugin-device" version="1.1.4" />
  <plugin name="cordova-plugin-dialogs" version="1.3.1" />
  <plugin name="cordova-plugin-file" version="4.3.1" />
  <plugin name="cordova-plugin-file-transfer" version="1.6.1" />
  <plugin name="cordova-plugin-zip" version="3.1.0" />
</widget>